  "lint-staged": {
    "**/*.{js,ts,vue}": "prettier --write",
    "*": "eslint --fix"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.23",
    "@nuxt/ui": "^2.18.7",
    "@vueuse/core": "^10.9.0",
    "chart.js": "^4.4.6",
    "date-fns": "^3.6.0",
    "nuxt": "^3.13.2",
    "radash": "^12.1.0",
    "steamid": "^2.1.0",
    "vlpt": "^0.0.1",
    "vue": "^3.5.12",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@nuxt/eslint": "^0.3.12",
    "@nuxt/fonts": "^0.3.0",
    "@nuxtjs/i18n": "^8.1.1",
    "@nuxtjs/tailwindcss": "^6.11.4",
    "eslint": "^9.2.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "openapi-typescript": "^7.4.1",
    "prettier": "3.2.5",
    "typescript": "^5.6.3"
  }
